





<div id="first">
    <br><br>
    <table>
        <thead>
        <tr>
            <th colspan="6" style="text-align:center">Market By Price</th>
        </tr>
        <tr>
            <th colspan="3" style="text-align:center">Buy Order </th>
            <th colspan="3" style="text-align:center">Sell Order</th>
        </tr>
        <tr>
            <th>#</th>
            <th>Volume</th>
            <th>Price</th>
            <th>Volume</th>
            <th>Price</th>
            <th>#</th>

        </tr>
        </thead>
        <tbody>
        {{#each item in populateTable}}
        <tr>
            <td>{{item.buyCount}}</td>
            <td style="text-align:center">{{item.buyShare}}</td>
            <td>{{item.buyPrice}}</td>
            <td>{{item.sellShare}}</td>
            <td>{{item.sellPrice}}</td>
            <td>{{item.sellCount}}</td>
        </tr>
        {{/each}}
        </tbody>
    </table>
</div>



<div id="second">
    <br><br>
    <table>
        <thead>
        <tr>
            <th colspan="4" style="text-align:center">Market By Order</th>
        </tr>
        <tr>
            <th colspan="2" style="text-align:center">Buy Order </th>
            <th colspan="2" style="text-align:center">Sell Order</th>
        </tr>
        <tr>

            <th>Volume</th>
            <th>Price</th>
            <th>Price</th>
            <th>Volume</th>

        </tr>
        </thead>
        {{#each item in getAll}}
        <tbody >
        <tr>
            <td style="text-align: center">{{item.buyS}}</td>
            <td style="text-align: center"> {{item.buyP}}</td>
            <td style="text-align: center" > {{item.sellP}}</td>
            <td style="text-align: center" > {{item.sellS}}</td>
        </tr>
        {{/each}}
        </tbody>


    </table>
</div>

<div id="third">
    <img {{bind-attr src=Logo}}>
</div>

